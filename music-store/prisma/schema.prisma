generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Author {
  id      String @id @default(uuid()) @db.Uuid
  name    String @db.VarChar(100)
  country String @db.VarChar(50)

  @@map("author")
}
